#Wrapper .pxl_leftColumn .ProductList .ProductPriceRating em {color: #4f2683 !important; font-weight: bold;}
#Wrapper .pxl_leftColumn .ProductList .ProductActionAdd {position: absolute; bottom: 10px; right: 10px; margin: 0; padding: 0; height: auto;}
#Wrapper .pxl_leftColumn .ProductList .ProductActionAdd a {display: block; margin: 0; padding: 0 !important; background: url(%%GLOBAL_TPL_PATH%%/images/btn23x23add-cart.png) no-repeat; width: 23px; height: 23px; text-indent: -9999px; background-color: transparent !important; border: 0;}
#Wrapper .pxl_leftColumn .ProductList .ProductActionAdd a:hover {display: block; margin: 0; padding: 0 !important; background: url(%%GLOBAL_TPL_PATH%%/images/btn23x23add-cart.png) no-repeat 0 -100px; width: 23px; height: 23px; text-indent: -9999px; background-color: transparent !important; border: 0;}

#Wrapper .pxl_rightColumn {margin-left: 500px; min-height: 872px; background-color: #fff;}
#Wrapper .pxl_rightColumn h2 {color: #4F2582; font-size: 22px; margin: 0; padding-bottom: 32px;}
#Wrapper .pxl_rightColumn p {color: #4F2582; font-size: 16px; padding-bottom: 6px;}

#Wrapper .pxl_rightColumn .pxl_introTxt {padding: 30px;}
#Wrapper .pxl_rightColumn .pxl_introTxt p{font-weight: 400 !important;color: #72519c !important;letter-spacing: normal !important;font-size: 16px !important;}

#Wrapper .pxl_circleButtons {position: relative; height: 160px; margin-top: 20px;}
#Wrapper .pxl_circleButtons ul {list-style: none; margin: 0; padding: 0;}
#Wrapper .pxl_circleButtons ul li {margin: 0; padding: 0; width: 130px; height: 130px; position: absolute; top: 10px; text-align: center; background: url(%%GLOBAL_TPL_PATH%%/images/bg130x130circleButton.png) no-repeat 0 0;}
#Wrapper .pxl_circleButtons ul li:hover {background-position: 0 -200px;}
#Wrapper .pxl_circleButtons ul li#pxl_circleButtonNews {left: 120px;}
#Wrapper .pxl_circleButtons ul li#pxl_circleButtonClasses {left: 425px;}
#Wrapper .pxl_circleButtons ul li#pxl_circleButtonComments {right: 120px;}
#Wrapper .pxl_circleButtons ul li a {display: block; padding: 40px 10px; color: #fff; font-size: 18px; text-decoration: none;}
#Wrapper .pxl_circleButtons ul li a:hover {color: #4f2683; text-decoration: none;}

#pxl_ContainerFooterWrapper {background: #f1eef5; margin-top: 20px; }
#ContainerFooter {padding-top: 0; background: transparent; float: none;}

#pxl_FooterTop {background: url(%%GLOBAL_TPL_PATH%%/images/bg_FooterTop.jpg) center top; padding-bottom: 20px;}
#pxl_FooterTop h2 {float: none; margin: 0 !important; padding: 30px 0 10px 0; font-size: 40px; text-transform: capitalize; color: #4F2582; font-family: 'OlicanaSmooth', 'Open Sans', 'Helvetica Neue', Helvetica, Arial, Sans-Serif; text-rendering: optimizeLegibility;}
#pxl_FooterTop p {color: #fff; font-size: 14px; margin: 0; padding: 3px 0;}
#pxl_FooterTop p a {color: #fff; text-decoration: none;}
#pxl_FooterTop p a:hover {color: #4F2582; text-decoration: none;}
#pxl_FooterTop p strong {font-weight: bold; font-size: 18px;}
