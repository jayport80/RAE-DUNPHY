#pxl_FooterTop .NewsletterColumn .NewsletterSubscription .BlockContent form button.go {float: none; margin: 0 !important; padding: 0 !important; display: block !important; position: absolute; bottom: 0; left: 320px; font-family: 'Open Sans', 'Helvetica Neue', Helvetica, Arial, Sans-Serif; text-rendering: optimizeLegibility;}
#pxl_FooterTop .NewsletterColumn .NewsletterSubscription .BlockContent form button.go span {display: block !important; width: auto !important; height: auto !important; margin: 0; padding: 3px 9px; background-image: none; text-indent: 0; background-color: #4F2582; color: #fff; font-size: 12px; font-weight: bold;}
#pxl_FooterTop .NewsletterColumn .NewsletterSubscription .BlockContent form button.go span:hover {background-color: #aed577; color: #4F2582;}
#pxl_FooterTop .pxl_ContactColumn {margin-left: 60%; text-align: center;}
#pxl_FooterTop .pxl_ContactColumn .SocialColumn #socnet {float: none; height: 40px; clear: none; padding: 15px 0 0 0; text-align: center;}
#pxl_FooterTop .pxl_ContactColumn .SocialColumn #socnet .icon-social-theme {background-image: url(%%GLOBAL_TPL_PATH%%/images/sprite-social.png); float: none !important; display: inline-block; width: 33px; height: 33px; margin: 0 6px;}

#pxl_FooterBottom {padding-top: 20px;}

#pxl_FooterBottom .Column {margin: 0; position: relative;}
#pxl_FooterBottom .Column div.Block {padding: 0 6px;}
#pxl_FooterBottom .Column div h2 {margin: 0; padding: 8px 0; color: #4F2582; font-size: 16px;}

#pxl_FooterBottom .Column div.Panel {}
#pxl_FooterBottom .Column div.Panel ul {margin: 0; padding: 0; list-style: none;}
#pxl_FooterBottom .Column div.Panel ul li {margin: 0; padding: 0; background-image: none; font-size: 12px; line-height: 160%;}
#pxl_FooterBottom .Column div.Panel ul li a {color: #4F2582; text-decoration: none;}
#pxl_FooterBottom .Column div.Panel ul li a:hover {color: #4F2582; text-decoration: underline;}
#pxl_FooterBottom .Column div.Panel ul li a span {}
#pxl_FooterBottom .Column div.Panel ul li a span ul {padding-left: 20px;}

#pxl_FooterBottom #SearchForm {position: absolute; top: 0; right: 0; width: 225px;}
#pxl_FooterBottom #SearchForm input {border: solid 1px #8f74b0; color: #4F2582; font-family: 'Open Sans', 'Helvetica Neue', Helvetica, Arial, Sans-Serif; font-weight: 300; text-rendering: optimizeLegibility; font-size: 12px; width: 160px; height: auto;}
#pxl_FooterBottom #SearchForm button {margin: 0 !important; height: auto;}
#pxl_FooterBottom #SearchForm button span {border: 0; width: 32px; height: 34px; margin: 0 !important; background-color: #8f74b0;}
#pxl_FooterBottom #SearchForm button:hover span {background-color: #4F2582;}

#pxl_FooterBottom .CategoryList .BlockContent > ul > ul.category-list > li > a {display: none;}
#pxl_FooterBottom .CategoryList .BlockContent > ul > ul.category-list > li > ul {display: block !important;}

#Footer p {font-size: 11px;color: #4F2582!important; text-align: right;}

