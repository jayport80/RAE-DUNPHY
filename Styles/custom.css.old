@charset "utf-8";

@import url(https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700);
* {font-family: 'Open Sans', 'Helvetica Neue', Helvetica, Arial, Sans-Serif; font-weight: 300; text-rendering: optimizeLegibility;}

@font-face {font-family: OlicanaSmooth; src: url("../fonts/OlicanaSmooth.otf") format("opentype");}

body {background: #f1eef5 url(%%GLOBAL_TPL_PATH%%/images/bg.jpg?v=2) no-repeat center top fixed;}

a {font-weight: inherit !important;}

.Textbox,
input,
textarea {border: solid 1px #8f74b0; color: #4F2582; font-family: 'Open Sans', 'Helvetica Neue', Helvetica, Arial, Sans-Serif; font-weight: 300; text-rendering: optimizeLegibility;}

.Content {padding-bottom: 0 !important;}
ul, ol { color: #72519c !important;font-size: 14px !important;}


.QuickViewBtn {background: #4f2683 !important;}
.QuickViewBtn:hover {background: #aed577 !important;}

#AjaxLoading {text-align: center;}
#ajax-loader {display: block; width: 64px; height: 64px; background:url(%%GLOBAL_TPL_PATH%%/images/loader64.gif) no-repeat; margin: 10px auto;}

/* home page and general fixes */

#Header {margin: 0; padding: 0; width: 980px; height: 260px;}

#TopMenu {margin: 0; padding: 0; height: 60px; background-color: transparent; border: 0; position: relative; top: 10px;}
#TopMenu ul li {background: url(%%GLOBAL_TPL_PATH%%/images/topMenuSep.png) 0 50% no-repeat;}
#TopMenu ul li,
#TopMenu ul li div {color: #fff; font-size: 11px;}
#TopMenu ul li #currency-chooser .currency-info {color: #4f2582;}
#TopMenu ul li #currency-chooser #change-currency {color: #fff; font-weight: 400;}
#TopMenu ul li #currency-chooser #change-currency:hover {color: #4f2582; text-decoration: underline;}
#TopMenu ul li a {color: #fff; font-weight: 400;}
#TopMenu ul li:hover a {color: #4f2582; font-weight: 400;}

#TopMenu .CartLink {background: url(%%GLOBAL_TPL_PATH%%/images/ico23x23cart.png) no-repeat 0 50%; padding-left: 28px;}
#TopMenu .CartLink a {background-image: none; padding: 0 4px !important; background-color: rgba(255, 255, 255, 0.6); border-radius: 4px; text-transform: uppercase; color: #4f2683; text-decoration: none !important;}
#TopMenu .CartLink a:hover {background-color: #4f2683; color: #fff; text-decoration: none !important;}

#HeaderUpper {margin: 0; padding: 0; height: 150px; background-color: transparent; border: 0;}
#HeaderUpper #Logo,
#HeaderUpper #Logo #LogoContainer,
#HeaderUpper #Logo #LogoContainer h1,
#HeaderUpper #Logo #LogoContainer h1 a {display: block; margin: 0; padding: 0; width: 100%; height: 150px;}
#HeaderUpper #Logo #LogoContainer h1 a {background: url(%%GLOBAL_TPL_PATH%%/images/logo-rda.png) no-repeat center center;}

#HeaderUpper #Logo #LogoContainer h1 a span {display: none;}

#HeaderLower {margin: 0; padding: 0; height: 50px; background-color: transparent; border: 0;}
#HeaderLower .Block {float: none;}
#HeaderLower .Block ul {width: auto;}
#HeaderLower .Block:after,
#HeaderLower .Block ul:after {content: ""; display: table; clear: both;} /* clearfix */
#HeaderLower .Block > ul > li:hover > a.hasSub {background: url(%%GLOBAL_TPL_PATH%%/images/dropdown-arrow.png) no-repeat 0 38px;}
#HeaderLower ul {text-align: center;}
#HeaderLower ul li {background: url(%%GLOBAL_TPL_PATH%%/images/menuSplit.png) 0 50% no-repeat; display: inline-block; float: none; color: #967db5;}
#HeaderLower ul li a {color: #4F2582; font-weight: normal!important;}
#HeaderLower ul li a span{color: #4F2582; font-weight: normal!important;}
#HeaderLower div > div > ul > li:hover a,
#HeaderLower div > div > ul > li a:hover {color: #4f2582; font-weight: 400;}

#HeaderLower ul ul li {border-bottom: solid 1px #fff;}
#HeaderLower ul ul li a {color: #fff !important; padding-right: 16px;}
#HeaderLower ul ul li:hover > a {background-color: #a7d269 !important;}
#HeaderLower ul ul li:hover > a,
#HeaderLower ul ul li a:hover {color: #4f2582 !important;}
#HeaderLower ul ul ul {left: 197px;}

#Wrapper {padding: 10px; min-height:550px;}

.flex-direction-nav a {background: url(%%GLOBAL_TPL_PATH%%/images/bg_direction_nav.png) no-repeat 0 0;}
.slide-show-render-full .flex-direction-nav a {width: 31px; height: 31px;}
.slide-show-render .flex-prev {background: url(%%GLOBAL_TPL_PATH%%/images/bg_direction_nav.png) no-repeat 0 0; left: -10px !important;}
.slide-show-render .flex-next {background: url(%%GLOBAL_TPL_PATH%%/images/bg_direction_nav.png) no-repeat -31px 0; right: -10px !important;}

.slide-show-render-full .slide-overlay {left: 80px; top: auto; bottom: 80px; text-align: left; margin: 0;}
.slide-show-render-full .slide-heading, 
.slide-show-render-full .slide-text {background: none; color: #fff;}
.slide-show-render-full .slide-heading {font-size: 40px;}
.slide-show-render-full .slide-text {font-size: 22px;}
.slide-show-render-full .slide-button {margin-top: 12px; text-align: left;}
.slide-show-render-full .slide-button span {background-color: #aed577; color: #fff; text-transform: uppercase; font-size: 14px; font-weight: bold;}
.slide-show-render-full .slide-button span:hover {background-color: #4f2683; color: #fff;}

.slide-show-render .slides,
.slide-show-render .slides li,
.slide-show-render .slides li img,
.slide-show-render-full .slide-content {height: 380px !important;}

#Wrapper {background-image: none;}

#Wrapper .pxl_leftColumn {float: left; width: 480px; min-height: 612px; background-color: #fff;}
#Wrapper .pxl_leftColumn h2 {color: #4F2582; font-size: 22px; margin: 0; padding-bottom: 32px;}
#Wrapper .pxl_leftColumn .Block {padding: 30px 30px 10px 30px; margin-bottom: 0;}
#Wrapper .pxl_leftColumn .Block .BlockContent {}
#Wrapper .pxl_leftColumn .ProductList {margin: 0; margin-right: -10px !important;}
#Wrapper .pxl_leftColumn .ProductList li {border: solid 1px #999; margin: 0 0 20px 0; width: 202px !important; position: relative;min-height: 239px;}
#Wrapper .pxl_leftColumn .ProductList li.Odd {margin-right: 20px;}
#Wrapper .pxl_leftColumn .ProductList .ProductImage,
#Wrapper .pxl_leftColumn .ProductList .ProductImage a  {border: 0; width: 200px !important; height: 160px !important; min-width: 200px !important; min-height: 160px !important;}
