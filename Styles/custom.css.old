#pxl_FooterTop .NewsletterColumn .NewsletterSubscription h2 {padding-left: 70px; margin-bottom: 0 !important; background: url(%%GLOBAL_TPL_PATH%%/images/ico_newsletter.png) no-repeat 10px 70%;}
#pxl_FooterTop .NewsletterColumn .NewsletterSubscription p {padding-left: 10px;}
#pxl_FooterTop .NewsletterColumn .NewsletterSubscription .BlockContent {margin-top: 10px; padding-left: 10px;}
